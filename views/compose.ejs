<%- include("partials/header") -%>

<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="contentTitle" autocomplete="off">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control" name="contentPost" rows="5" autocomplete="off"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>


<%- include("partials/footer") -%>
